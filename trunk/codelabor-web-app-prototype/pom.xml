		<!-- DBMS profiles -->
		<profile>
			<id>mysql</id>
			<properties>
				<database.server>mysql</database.server>
			</properties>
			<dependencies>
				<dependency>
					<groupId>mysql</groupId>
					<artifactId>mysql-connector-java</artifactId>
					<version>${mysql.version}</version>
					<scope>test</scope>
				</dependency>
			</dependencies>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>sql-maven-plugin</artifactId>
						<version>1.5</version>
						<dependencies>
							<dependency>
								<groupId>mysql</groupId>
								<artifactId>mysql-connector-java</artifactId>
								<version>${mysql.version}</version>
							</dependency>
						</dependencies>
						<configuration>
							<driver>com.mysql.jdbc.Driver</driver>
							<url>jdbc:mysql://localhost:3306/test</url>
							<username>scott</username>
							<password>tiger</password>
						</configuration>
						<executions>
							<execution>
								<id>drop-table</id>
								<phase>process-test-resources</phase>
								<goals>
									<goal>execute</goal>
								</goals>
								<configuration>
									<autocommit>true</autocommit>
									<srcFiles>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/drop-emp-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/drop-dept-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/drop-salgrade-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/drop-bonus-table.sql</srcFile>
									</srcFiles>
									<onError>continue</onError>
								</configuration>
							</execution>
							<execution>
								<id>create-table</id>
								<phase>process-test-resources</phase>
								<goals>
									<goal>execute</goal>
								</goals>
								<configuration>
									<autocommit>true</autocommit>
									<srcFiles>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/create-bonus-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/create-salgrade-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/create-dept-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/ddl/create-emp-table.sql</srcFile>
									</srcFiles>
									<onError>continue</onError>
								</configuration>
							</execution>
							<execution>
								<id>insert-data</id>
								<phase>process-test-resources</phase>
								<goals>
									<goal>execute</goal>
								</goals>
								<configuration>
									<autocommit>true</autocommit>
									<srcFiles>
										<srcFile>${basedir}/src/test/sql/mysql/example/dml/insert-bonus-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/dml/insert-salgrade-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/dml/insert-dept-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/example/dml/insert-emp-table.sql</srcFile>
									</srcFiles>
									<onError>continue</onError>
								</configuration>
							</execution>

							<!-- spring security -->
							<execution>
								<id>drop-spring-security-tables</id>
								<phase>process-test-resources</phase>
								<goals>
									<goal>execute</goal>
								</goals>
								<configuration>
									<autocommit>true</autocommit>
									<srcFiles>
										<srcFile>${basedir}/src/test/sql/mysql/system/ddl/drop-persistent-logins-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/system/ddl/drop-acl-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/system/ddl/drop-authorities-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/system/ddl/drop-group-table.sql</srcFile>
										<srcFile>${basedir}/src/test/sql/mysql/system/ddl/drop-user-table.sql</srcFile>
									</srcFiles>
									<onError>continue</onError>
								</configuration>
							</execution>
							<execution>
